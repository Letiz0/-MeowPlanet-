@model IEnumerable<MeowPlanet.ViewModels.MemberInfo.CatSelectViewModel>



@foreach (var item in Model)
{
    
    string sex = item.Sex == true ? "男生" : "女生";
    

    <div data-id="@item.CatId"class="cat-select d-flex align-items-center px-4" style="cursor:pointer;" onclick="ToDetail(this.dataset.id)">
                
        <img src="@item.Img01" style="width:150px;height:110px;border-radius:30px;" >

        <div class="d-flex align-items-center justify-content-center" style="width:219px;height:110px;">
            <p  style="font-size:25px">@item.Name</p>
            <p  style="font-size:25px">@item.Breed</p>
            <p  style="font-size:25px">@sex</p>
        </div>
    </div>

}

